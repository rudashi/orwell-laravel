    "name": "rudashi/orwell-laravel",
    "license": "proprietary",
    "authors": [
        {
            "name": "Borys Å»muda",
            "email": "rudashi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations/",
            "src/database/seeders/"
        ],
        "psr-4": {
            "Rudashi\\Orwell\\": "src/",
            "Rudashi\\Orwell\\Database\\Seeders\\": "src/database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rudashi\\Orwell\\OrwellServiceProvider"
            ]
        }
    }
}
