{
    "name": "rudashi/orwell-laravel",
    "description": "Free, open-source and dedicated for Laravel package for Scrabble or Literaki. Finds words using given letters. Available only in Polish.",
    "keywords": [
        "laravel",
        "library",
        "words",
        "word",
        "scrabble",
        "scrabble-dictionary",
        "scrabble-solver",
        "scrabble-game",
        "scrabble-word-finder",
        "literaki"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Borys Å»muda",
            "email": "rudashi@gmail.com",
            "homepage": "https://rudashi.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^v2.0"
    },
    "autoload": {
        "psr-4": {
            "Rudashi\\Orwell\\": "src/",
            "Rudashi\\Orwell\\Database\\Seeders\\": "src/database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rudashi\\Orwell\\OrwellServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
